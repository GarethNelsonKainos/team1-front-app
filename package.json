    "@playwright/test": "^1.58.2",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^25.2.2",
    "@types/nunjucks": "^3.2.6",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "axios": "^1.13.5",
    "nodemon": "^3.1.11",
    "pa11y": "^9.1.1",
    "pa11y-ci": "^4.0.1",
    "supertest": "^6.3.4",
