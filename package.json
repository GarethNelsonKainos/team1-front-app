{
  "name": "team1-front-app",
  "version": "1.0.0",
  "description": "Team 1 Frontend Application Feb/March 2026",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:a11y:axe": "playwright test tests/e2e/accessibility.spec.ts",
    "test:a11y:pa11y": "pa11y-ci",
    "test:a11y:report": "playwright test tests/e2e/accessibility.spec.ts --reporter=html",
    "test:cucumber": "NODE_OPTIONS='--import tsx' cucumber-js",
    "check": "biome check .",
    "check:fix": "biome check . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarethNelsonKainos/team1-front-app.git"
  },
